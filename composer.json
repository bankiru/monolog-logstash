    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "phpunit/phpunit-mock-objects": "~2.3",
        "phpunit/phpcov": "~2.0",
        "fabpot/php-cs-fixer": "~1.5",
        "satooshi/php-coveralls": "~0.6"
    },
    "autoload": {
        "psr-4": {
            "Bankiru\\MonologLogstash\\": "src/"
        }
    }
}
