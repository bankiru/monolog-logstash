{
    "name": "bankiru/monolog-logstash",
    "description": "Monolog Logstash - logging to Logstash",
    "license": "MIT",
    "authors": [
        {
            "name": "Boris Gorbylev",
            "email": "gorbylev@banki.ru"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "monolog/monolog": "~1.12",
